	<script type='text/javascript' src="files/js/pace.min.js"></script>
	<script type='text/javascript' src="files/js/compiled_all.js"></script>
	<script type='text/javascript'>
		$(document).ready(function() {
			Application.init();

			window.onscroll = function () { 
				if(settings.transitioning)
					window.scrollTo(0, 0);
			};

			randomAward();


			$(function() {
			    Pace.on("done", function(){

			    	if(settings.currentSection == '/') {
			    		logoAnimation();
			    	} else {
			    		Application.parallax();
			        	$('.site').fadeIn(600);
			    	}
			        //console.log("DONE LOADING!");
			        Application.resize();

			        if($projectOpen) {
			        	
			        	ProjectView.transition();
			        	TweenMax.to($('.pace-progress'), .5, {width:0, delay:.2, ease:Power2.easeOut, onComplete:function() {

			        	}});
		
			        	//$('.pace').fadeOut(300);
			        	
			        } else {
			        	$('.pace').fadeOut(300);
			        	preloadProjectImages();
			        }

			    });
			});

			function randomAward() {

			
				$awwwards = jQuery('<div id="awwwards" class="honorable white left"><a href="https://www.awwwards.com/best-websites/animal/" target="_blank">Awwwards</a></div>');
				$cssda = jQuery('<div id="cssda" class="white left"><a href="https://www.cssdesignawards.com/sites/animal/27970/" target="_blank">CSS Design Awards</a></div>');

				var array = [];
				array.push($awwwards);
				array.push($cssda);

				var n = Math.floor(Math.random() * array.length);
				$randAward = array[n];
				$randAward.appendTo('#awards');

			}

			function preloadProjectImages() {

				for(var i = 0; i < settings.portData.projects.length; i++){

				    var project = settings.portData.projects[i];

					$heroURL = project.hero;
					$hero = jQuery('<img src="'+settings.cdnPath+$heroURL+'" width="1" height="1" alt="Hero Image" />');
					$hero.appendTo('#preload');

					$logoURL = project.logo;
					$logo = jQuery('<img src="'+settings.cdnPath+$logoURL+'" width="1" height="1" alt="Logo" />');
					$logo.appendTo('#preload');

					$thumbURL = project.thumb;
					$thumb = jQuery('<img src="'+settings.cdnPath+$thumbURL+'" width="1" height="1" alt="Thumb Image" />');
					$thumb.appendTo('#preload');
				};
				

			};

			function logoAnimation() {
				$('#preload-logo-anim').addClass('active');

				$('body').bind('touchmove', function(e){e.preventDefault()});

				settings.transitioning = true;

				var animationEvent = Application.whichAnimationEvent();
	
				$('#logo-intro').one(animationEvent,
					function(event) {
						$('.site').fadeIn(600, function() {
							$('#preload-logo-anim').fadeOut(400, function() {
								$('#preload-logo-anim').removeClass('active');
								Application.parallax();
								settings.transitioning = false;
								$('body').unbind('touchmove');
							});

						});
					});

			};

		});
	</script>
</body>
</html>